<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title> {{>title}} StartUp Financing Webapp</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    {{#each css}}
        <link rel="stylesheet" type="text/css" media="screen" href="/css/{{this}}" />
    {{/each}}
    {{!-- <link rel="stylesheet" type="text/css" media="screen" href="/css/main.css" /> --}}
    <link rel="shortcut icon" href="/custom.ico" type="image/x-icon" />
    <script src="public/main.js"></script>
</head>
<!-- This webpage belongs to the signed in Entrepreneur -->
<body class="logged-out">
  {{>guest_header port=port}}
        <div class="row">
        <div class="col-lg-12">
          {{#if success_msg}}
            <div class="alert alert-success">{{success_msg}}</div>
          {{/if}}

          {{#if error_msg}}
            <div class="alert alert-danger">{{error_msg}}</div>
          {{/if}}

          {{#if error}}
            <div class="alert alert-danger">{{error}}</div>
          {{/if}}
          
          {{{body}}}
        </div>

        
</div>
  
  
</body>
</html>